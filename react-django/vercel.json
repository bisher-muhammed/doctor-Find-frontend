{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/socket.io/(.*)",
      "headers": [
        { "key": "Connection", "value": "Upgrade" },
        { "key": "Upgrade", "value": "websocket" }
      ]
    }
  ]
}